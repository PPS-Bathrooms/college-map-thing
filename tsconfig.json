{
  "compilerOptions": {
    "module": "commonjs",
    "target": "es2020",
    "jsx": "react",
    "esModuleInterop": true,
    "sourceMap": true,
    "outDir": "dist",
    "plugins": [
      { "name": "dotenv/typescript" }
    ]
  },
  "include": ["src/**/*.tsx", "src/**/*.ts"],
  "exclude": ["node_modules"]
}
